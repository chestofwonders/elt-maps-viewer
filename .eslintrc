{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": { 
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "curly": "error", 
        "no-empty": "error",
        "@typescript-eslint/no-explicit-any": ["error", {  "fixToUnknown": true}]
    }
  }